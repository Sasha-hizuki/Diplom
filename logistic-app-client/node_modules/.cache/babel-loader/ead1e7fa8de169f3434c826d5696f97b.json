A;AAAA;AAAA;AAeA,KAzCA;AA0CA,IAAA,OA1CA,qBA0CA;AACA,WAAA,IAAA,GAAA;AAAA,QAAA,EAAA,EAAA,IAAA;AAAA,QAAA,KAAA,EAAA,IAAA;AAAA,QAAA,KAAA,EAAA,IAAA;AAAA,QAAA,MAAA,EAAA,IAAA;AAAA,QAAA,MAAA,EAAA,IAAA;AAAA,QAAA,gBAAA,EAAA,IAAA;AAAA,QAAA,eAAA,EAAA;AAAA,OAAA;AACA,WAAA,SAAA;AACA,KA7CA;AA8CA,IAAA,SA9CA,uBA8CA;AACA,WAAA,IAAA,GAAA,IAAA;AACA,KAhDA;AAiDA,IAAA,UAjDA,wBAiDA;AACA,WAAA,IAAA,GAAA,KAAA;AACA,WAAA,IAAA,GAAA;AAAA,QAAA,EAAA,EAAA,IAAA;AAAA,QAAA,KAAA,EAAA,IAAA;AAAA,QAAA,KAAA,EAAA,IAAA;AAAA,QAAA,MAAA,EAAA,IAAA;AAAA,QAAA,MAAA,EAAA,IAAA;AAAA,QAAA,gBAAA,EAAA,IAAA;AAAA,QAAA,eAAA,EAAA;AAAA,OAAA;AACA,KApDA;AAqDA,IAAA,QArDA,sBAqDA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,qBACA,MAAA,CAAA,IAAA,CAAA,EADA;AAAA;AAAA;AAAA;;AAAA;AAAA,uBAEA,KAAA,CAAA,yCAAA,MAAA,CAAA,IAAA,CAAA,EAAA,EACA;AACA,kBAAA,MAAA,EAAA,KADA;AAEA,kBAAA,IAAA,EAAA,IAAA,CAAA,SAAA,CAAA,MAAA,CAAA,IAAA,CAFA;AAGA,kBAAA,OAAA,EAAA;AACA,oCAAA,kBADA;AAEA,qCAAA,YAAA,YAAA,CAAA,OAAA,CAAA,KAAA;AAFA;AAHA,iBADA,CAAA,CASA,OATA,CASA,YAAA;AACA,kBAAA,MAAA,CAAA,QAAA;;AACA,kBAAA,MAAA,CAAA,UAAA;AACA,iBAZA,CAFA;;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA,uBAgBA,KAAA,CAAA,sCAAA,EACA;AACA,kBAAA,MAAA,EAAA,MADA;AAEA,kBAAA,IAAA,EAAA,IAAA,CAAA,SAAA,CAAA,MAAA,CAAA,IAAA,EAAA,UAAA,GAAA,EAAA,KAAA,EAAA;AACA,wBAAA,GAAA,KAAA,SAAA,IAAA,GAAA,KAAA,kBAAA,EAAA;AACA,6BAAA,UAAA,CAAA,KAAA,CAAA;AACA;;AACA,2BAAA,KAAA;AACA,mBALA,CAFA;AAQA,kBAAA,OAAA,EAAA;AACA,oCAAA,kBADA;AAEA,qCAAA,YAAA,YAAA,CAAA,OAAA,CAAA,KAAA;AAFA;AARA,iBADA,CAAA,CAcA,IAdA,CAcA,UAAA,QAAA;AAAA,yBAAA,QAAA,CAAA,IAAA,EAAA;AAAA,iBAdA,EAeA,OAfA,CAeA,YAAA;AACA,kBAAA,MAAA,CAAA,QAAA;;AACA,kBAAA,MAAA,CAAA,UAAA;AACA,iBAlBA,CAhBA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAqCA;AA1FA,GAnBA;AA+GA,EAAA,OA/GA,qBA+GA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,cAAA,MAAA,CAAA,QAAA;;AADA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAEA,GAjHA;AAkHA,EAAA,UAAA,EAAA;AACA,IAAA,MAAA,EAAA,MADA;AACA,IAAA,OAAA,EAAA;AADA;AAlHA,CAAA","sourcesContent":["<template>\n    <div class=\"list\">\n        <h1 class=\"title\">Прицепы</h1>\n        <input class=\"field__search form-control form-control-sm\" type=\"text\" placeholder=\"Поиск\" v-model=\"search\">        \n        <button class=\"btn btn_icon btn-sm btn-secondary new__btn\"  v-on:click.prevent=\"newItem\"><i>&#43;</i><span>Добавить</span></button>\n        <Loader v-if=\"loading\"/>\n        <ul class=\"items-list\" v-else-if=\"searchItems.length\">\n            <Trailer\n                v-for=\"(item, i) of searchItems\" :key=\"item.id\"\n                v-bind:item=\"item\" \n                v-bind:index=\"i\"\n                @remove-item=\"removeItem\"\n                @get-item=\"getItem\"\n            />\n        </ul>\n        <p v-else>Список пуст</p>\n        <div v-if=\"Form\" class=\"modal\">\n            <form id=\"From\" class=\"modal__form\" @submit.prevent=\"onSubmit\">           \n                <input v-if=\"item.id\" v-model=\"item.id\" type=\"hidden\">\n                <div class=\"form-row\">\n                  <div class=\"col form-group\">\n                    <label for=\"brand\">Брэнд</label>\n                    <input class=\"form-control\" name=\"brand\" required v-model=\"item.brand\" type=\"text\">\n                  </div>\n                  <div class=\"col form-group\">\n                    <label for=\"model\">Модель</label>\n                    <input class=\"form-control\" name=\"model\" v-model=\"item.model\" type=\"text\">\n                  </div>\n                </div>\n                <div class=\"form-row\">\n                  <div class=\"col form-group\">\n                    <label for=\"number\">Номер</label>\n                    <input class=\"form-control\" name=\"number\" v-model=\"item.number\" type=\"text\">\n                  </div>\n                  <div class=\"col form-group\">\n                    <label for=\"TODate\">Дата последнего ТО</label>\n                    <input class=\"form-control\" name=\"TODate\" required v-model=\"item.toDate\" type=\"date\">\n                  </div>\n                </div>\n                <div class=\"form-row\">\n                  <div class=\"col form-group\">\n                    <label for=\"BlrInsuranceDate\">Дата бел. страховки</label>\n                    <input class=\"form-control\" name=\"BlrInsuranceDate\" required v-model=\"item.blrInsuranceDate\" type=\"date\">\n                  </div>\n                  <div class=\"col form-group\">\n                    <label for=\"GCInsuranceDate\">Дата зеленой карты</label>\n                    <input class=\"form-control\" name=\"GCInsuranceDate\" required v-model=\"item.gcInsuranceDate\" type=\"date\">\n                  </div>\n                </div>\n                <input class=\"btn btn-primary\" type=\"submit\" value=\"Сохранить\">\n                <button class=\"modal__form__close\" v-on:click.prevent=\"closeModal\"><svg xmlns=\"http://www.w3.org/2000/svg\" version=\"1\" viewBox=\"0 0 24 24\"><path d=\"M13 12l5-5-1-1-5 5-5-5-1 1 5 5-5 5 1 1 5-5 5 5 1-1z\"></path></svg></button>\n            </form>\n        </div>\n    </div>\n</template>\n\n<script>\nimport Loader from '@/components/Loader'\nimport Trailer from '@/components/Trailer'\nexport default {\n    data(){\n        return {\n            ItemsList:[],\n            loading: true,\n            Form:false,\n            item:{id:null,brand:null,model:null,number:null,toDate:null,blrInsuranceDate:null,gcInsuranceDate:null},\n            search:\"\"\n        }\n    },\n    computed:{\n        searchItems(){\n            if(this.search.length){\n                return this.ItemsList.filter(c => c.brand.toLowerCase().startsWith(this.search.toLowerCase()) || c.model.toLowerCase().startsWith(this.search.toLowerCase()) || c.number.toLowerCase().startsWith(this.search.toLowerCase()))\n            }else{\n                return this.ItemsList\n            }\n        }\n    },\n    methods:{\n        async removeItem(id){\n            await fetch('https://localhost:5001/api/trailers/'+id, { \n                method: 'DELETE',\n                headers: {\n                    'Content-Type': 'application/json',\n                    'Cache-Control': 'no-cache',\n                    'Authorization': 'Bearer '+ localStorage.getItem('jwt')\n                }\n            })\n            this.ItemsList = this.ItemsList.filter(c => c.id !== id)\n        },\n        async getItems(){\n            await fetch('https://localhost:5001/api/trailers/',{\n               headers: {\n                    'Content-Type': 'application/json',\n                    'Cache-Control': 'no-cache',\n                    'Authorization': 'Bearer '+ localStorage.getItem('jwt')\n                }\n           })\n            .then(response => response.json())\n            .then(json => {\n                this.ItemsList = json                \n                this.loading = false\n            })\n        },\n        async getItem(id){\n            await fetch('https://localhost:5001/api/trailers/'+id,{\n               headers: {\n                    'Content-Type': 'application/json',\n                    'Cache-Control': 'no-cache',\n                    'Authorization': 'Bearer '+ localStorage.getItem('jwt')\n                }\n            })\n            .then(response => response.json())\n            .then(json => {\n                this.item = json;\n                this.item.toDate = this.item.toDate.split('T')[0];\n                this.item.blrInsuranceDate = this.item.blrInsuranceDate.split('T')[0];\n                this.item.gcInsuranceDate = this.item.gcInsuranceDate.split('T')[0];\n            }).finally(()=>this.openModal())\n        },\n        newItem(){\n            this.item = {id:null,brand:null,model:null,number:null,toDate:null,blrInsuranceDate:null,gcInsuranceDate:null};\n            this.openModal()\n        },\n        openModal(){\n            this.Form = true;\n        },\n        closeModal(){\n            this.Form = false;\n            this.item = {id:null,brand:null,model:null,number:null,toDate:null,blrInsuranceDate:null,gcInsuranceDate:null};\n        },\n        async onSubmit(){\n            if(this.item.id){\n                await fetch('https://localhost:5001/api/trailers/'+this.item.id, \n                { \n                    method: 'PUT',\n                    body: JSON.stringify(this.item),\n                    headers: {\n                        'Content-Type': 'application/json',\n                        'Authorization': 'Bearer '+ localStorage.getItem('jwt')\n                    }\n                })\n                .finally(() => {                    \n                    this.getItems() \n                    this.closeModal()\n                })\n            }else{\n                await fetch('https://localhost:5001/api/trailers/', \n                { \n                    method: 'POST',\n                    body: JSON.stringify(this.item, function(key, value){\n                        if(key === \"mileage\" || key === \"fuel_consumption\"){\n                            return parseFloat(value)\n                        }                        \n                        return value\n                    }),\n                    headers: {\n                        'Content-Type': 'application/json',\n                        'Authorization': 'Bearer '+ localStorage.getItem('jwt')\n                    }\n                })\n                .then(response => response.json())\n                .finally(() => {                    \n                    this.getItems() \n                    this.closeModal()\n                })\n            }\n            \n        }\n    },\n    async mounted(){\n        this.getItems()\n    },\n    components:{\n        Loader, Trailer\n    } \n}\n</script>\n\n<style lang=\"scss\">\n  \n.list {\n  \n  .new__btn {\n    margin-bottom: 8px;\n  }\n  \n  .field__search {\n    margin-bottom: 1.2rem;\n  }\n  \n  .title {\n    margin-bottom: 1.5rem;\n  }\n}\n.modal {\n  position: absolute;\n  top: 20%;\n  left: 50%;\n  transform: translateY(-20%) translatex(-50%);\n  width: 500px;\n  height: auto;\n  border: 1px solid #EBEBFF;\n  border-radius: 0.3rem;\n  background-color: $white;\n  padding: 1.5rem;\n  \n  &__form {\n    &__close {\n      position: absolute;\n      opacity: .8;\n      padding: 10px;\n      right: 0;\n      top: 0;\n      cursor: pointer;\n      background: transparent;\n      border: 0;\n      border-radius: 0;\n      box-shadow: none;\n      display: inline-block;\n      height: 44px;\n      vertical-align: top;\n      visibility: inherit;\n      width: 44px;\n      \n      &:hover {\n        opacity: 1;\n      }\n      \n      &:focus {\n        outline: none;\n      }\n    }\n  }\n}\n</style>"],"sourceRoot":"src/views"}]}