CA,uBA6CA;AACA,WAAA,IAAA,GAAA,IAAA;AACA,KA/CA;AAgDA,IAAA,UAhDA,wBAgDA;AACA,WAAA,IAAA,GAAA,KAAA;AACA,WAAA,IAAA,GAAA;AAAA,QAAA,EAAA,EAAA,IAAA;AAAA,QAAA,GAAA,EAAA,IAAA;AAAA,QAAA,IAAA,EAAA,IAAA;AAAA,QAAA,QAAA,EAAA,IAAA;AAAA,QAAA,aAAA,EAAA;AAAA,OAAA;AACA,WAAA,YAAA,GAAA,IAAA;AACA,KApDA;AAqDA,IAAA,QArDA,sBAqDA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,qBACA,MAAA,CAAA,IAAA,CAAA,EADA;AAAA;AAAA;AAAA;;AAAA;AAAA,uBAEA,KAAA,CAAA,sCAAA,MAAA,CAAA,IAAA,CAAA,EAAA,EACA;AACA,kBAAA,MAAA,EAAA,KADA;AAEA,kBAAA,IAAA,EAAA,IAAA,CAAA,SAAA,CAAA,MAAA,CAAA,IAAA,EAAA,UAAA,GAAA,EAAA,KAAA,EAAA;AACA,wBAAA,GAAA,KAAA,eAAA,EAAA;AACA,6BAAA;AAAA,8BAAA,UAAA,CAAA,KAAA,CAAA,EAAA;AAAA,uBAAA;AACA;;AACA,2BAAA,KAAA;AACA,mBALA,CAFA;AAQA,kBAAA,OAAA,EAAA;AACA,oCAAA,kBADA;AAEA,qCAAA,YAAA,YAAA,CAAA,OAAA,CAAA,KAAA;AAFA;AARA,iBADA,CAAA,CAcA,OAdA,CAcA,YAAA;AACA,kBAAA,MAAA,CAAA,QAAA;;AACA,kBAAA,MAAA,CAAA,UAAA;AACA,iBAjBA,CAFA;;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA,uBAqBA,KAAA,CAAA,mCAAA,EACA;AACA,kBAAA,MAAA,EAAA,MADA;AAEA,kBAAA,IAAA,EAAA,IAAA,CAAA,SAAA,CAAA,MAAA,CAAA,IAAA,EAAA,UAAA,GAAA,EAAA,KAAA,EAAA;AACA,wBAAA,GAAA,KAAA,eAAA,EAAA;AACA,6BAAA;AAAA,8BAAA,UAAA,CAAA,KAAA;AAAA,uBAAA;AACA;;AACA,2BAAA,KAAA;AACA,mBALA,CAFA;AAQA,kBAAA,OAAA,EAAA;AACA,oCAAA,kBADA;AAEA,qCAAA,YAAA,YAAA,CAAA,OAAA,CAAA,KAAA;AAFA;AARA,iBADA,CAAA,CAcA,IAdA,CAcA,UAAA,QAAA;AAAA,yBAAA,QAAA,CAAA,IAAA,EAAA;AAAA,iBAdA,EAeA,OAfA,CAeA,YAAA;AACA,kBAAA,MAAA,CAAA,QAAA;;AACA,kBAAA,MAAA,CAAA,UAAA;AACA,iBAlBA,CArBA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AA0CA;AA/FA,GArCA;AAsIA,EAAA,OAtIA,qBAsIA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,cAAA,MAAA,CAAA,QAAA;;AADA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAEA,GAxIA;AAyIA,EAAA,UAAA,EAAA;AACA,IAAA,MAAA,EAAA,MADA;AACA,IAAA,IAAA,EAAA;AADA;AAzIA,CAAA","sourcesContent":["<template>\n    <div class=\"list\">\n        <h1 class=\"title\">Пользователи</h1>\n        <input class=\"field__search form-control form-control-sm\" type=\"text\" placeholder=\"Поиск\" v-model=\"search\">        \n        <button class=\"btn btn_icon btn-sm btn-secondary new__btn\"  v-on:click.prevent=\"newItem\"><i>&#43;</i><span>Добавить</span></button>\n        <Loader v-if=\"loading\"/>\n        <ul class=\"items-list\" v-else-if=\"searchItems.length\">\n            <User\n                v-for=\"(item, i) of searchItems\" :key=\"item.id\"\n                v-bind:item=\"item\" \n                v-bind:index=\"i\"\n                @remove-item=\"removeItem\"\n                @get-item=\"getItem\"\n            />\n        </ul>\n        <p v-else>Список пуст</p>\n        <div v-if=\"Form\" class=\"modal\">\n            <form id=\"From\" class=\"modal__form\" @submit.prevent=\"onSubmit\">           \n                <input v-if=\"item.id\" v-model=\"item.id\" type=\"hidden\">\n                <div class=\"form-row\">\n                    <div class=\"col form-group\">\n                        <label for=\"brand\">Имя</label>\n                        <input class=\"form-control\" name=\"brand\" required v-model=\"item.fio\" type=\"text\">\n                    </div>\n                </div>\n                <div class=\"form-row\">                  \n                  <div class=\"col form-group\">\n                    <label for=\"brand\">Логин</label>\n                    <input class=\"form-control\" name=\"brand\" required v-model=\"item.name\" type=\"text\">\n                  </div>\n                  <div class=\"col form-group\">\n                    <label for=\"model\">Пароль</label>\n                    <input class=\"form-control\" name=\"model\" required v-model=\"item.password\" type=\"password\">\n                  </div>\n                </div>\n                <div class=\"form-row\">\n                  <div class=\"col form-group\">\n                    <label for=\"fuel_consumption\">Роль</label>\n                    <select v-if=\"item.id\" required class=\"form-control\" v-model=\"item.user_position.id\">\n                        <option disabled :value=\"null\" selected hidden>Выберите один из вариантов</option>\n                        <option v-for=\"pos of userPositionsList\" :key=\"pos.id\" :value=pos.id>{{pos.title}}</option>\n                    </select>\n                    <select v-else required class=\"form-control\" v-model=\"item.user_position\">\n                        <option disabled :value=\"null\" selected hidden>Выберите один из вариантов</option>\n                        <option v-for=\"pos of userPositionsList\" :key=\"pos.id\" :value=pos.id>{{pos.title}}</option>\n                    </select>\n                  </div>\n                </div>\n                <input class=\"btn btn-primary\" type=\"submit\" value=\"Сохранить\">\n                <button class=\"modal__form__close\" v-on:click.prevent=\"closeModal\"><svg xmlns=\"http://www.w3.org/2000/svg\" version=\"1\" viewBox=\"0 0 24 24\"><path d=\"M13 12l5-5-1-1-5 5-5-5-1 1 5 5-5 5 1 1 5-5 5 5 1-1z\"></path></svg></button>\n            </form>\n        </div>\n    </div>\n</template>\n\n<script>\nimport Loader from '@/components/Loader'\nimport User from '@/components/User'\nexport default {\n    data(){\n        return {\n            ItemsList:[],\n            loading: true,\n            Form:false,\n            item:{id:null,fio:null,name:null,password:null,user_position:null},\n            search:\"\",\n            userPositionsList:[\n                {\n                    id:1,\n                    name:\"Admin\",\n                    title:\"Директор\"\n                },\n                {\n                    id:2,\n                    name:\"Driver\",\n                    title:\"Бугалтер\"\n                },\n                {\n                    id:3,\n                    name:\"Logist\",\n                    title:\"Логист\"\n                }\n                \n            ]\n        }\n    },\n    computed:{\n        searchItems(){\n            if(this.search.length){\n                return this.ItemsList.filter(c => c.fio.toLowerCase().startsWith(this.search.toLowerCase()))\n            }else{\n                return this.ItemsList\n            }\n        }\n    },\n    methods:{\n        async removeItem(id){\n            await fetch('https://localhost:5001/api/users/'+id, { \n                method: 'DELETE',\n                headers: {\n                    'Content-Type': 'application/json',\n                    'Cache-Control': 'no-cache',\n                    'Authorization': 'Bearer '+ localStorage.getItem('jwt')\n                }\n            })\n            this.ItemsList = this.ItemsList.filter(c => c.id !== id)\n        },\n        async getItems(){\n            await fetch('https://localhost:5001/api/users/',{\n               headers: {\n                    'Content-Type': 'application/json',\n                    'Cache-Control': 'no-cache',\n                    'Authorization': 'Bearer '+ localStorage.getItem('jwt')\n                }\n           })\n            .then(response => response.json())\n            .then(json => {\n                this.ItemsList = json                \n                this.loading = false\n            })\n        },\n        async getItem(id){\n            await fetch('https://localhost:5001/api/users/'+id,{\n               headers: {\n                    'Content-Type': 'application/json',\n                    'Cache-Control': 'no-cache',\n                    'Authorization': 'Bearer '+ localStorage.getItem('jwt')\n                }\n            })\n            .then(response => response.json())\n            .then(json => {\n                this.item = json;\n            }).finally(\n                ()=>this.openModal()\n            )\n        },\n        newItem(){\n            this.item = {id:null,fio:null,name:null,password:null,user_position:null};\n            this.openModal()\n        },\n        openModal(){\n            this.Form = true;\n        },\n        closeModal(){\n            this.Form = false;\n            this.item = {id:null,fio:null,name:null,password:null,user_position:null};\n            this.selectedItem = null;\n        },\n        async onSubmit(){\n            if(this.item.id){\n                await fetch('https://localhost:5001/api/users/'+this.item.id, \n                { \n                    method: 'PUT',\n                    body: JSON.stringify(this.item, function(key, value){\n                        if(key === \"user_position\"){\n                            return {\"id\":parseFloat(value.id)}\n                        }                        \n                        return value\n                    }),\n                    headers: {\n                        'Content-Type': 'application/json',\n                        'Authorization': 'Bearer '+ localStorage.getItem('jwt')\n                    }\n                })\n                .finally(() => {                    \n                    this.getItems() \n                    this.closeModal()\n                })\n            }else{\n                await fetch('https://localhost:5001/api/users/', \n                { \n                    method: 'POST',\n                    body: JSON.stringify(this.item, function(key, value){\n                        if(key === \"user_position\"){\n                            return {\"id\":parseFloat(value)}\n                        }                        \n                        return value\n                    }),\n                    headers: {\n                        'Content-Type': 'application/json',\n                        'Authorization': 'Bearer '+ localStorage.getItem('jwt')\n                    }\n                })\n                .then(response => response.json())\n                .finally(() => {                    \n                    this.getItems() \n                    this.closeModal()\n                })\n            }\n            \n        }\n    },\n    async mounted(){\n        this.getItems()\n    },\n    components:{\n        Loader, User\n    } \n}\n</script>\n\n<style lang=\"scss\">\n  \n.list {\n  \n  .new__btn {\n    margin-bottom: 8px;\n  }\n  \n  .field__search {\n    margin-bottom: 1.2rem;\n  }\n  \n  .title {\n    margin-bottom: 1.5rem;\n  }\n}\n.modal {\n  position: absolute;\n  top: 20%;\n  left: 50%;\n  transform: translateY(-20%) translatex(-50%);\n  width: 500px;\n  height: auto;\n  border: 1px solid #EBEBFF;\n  border-radius: 0.3rem;\n  background-color: $white;\n  padding: 1.5rem;\n  \n  &__form {\n    &__close {\n      position: absolute;\n      opacity: .8;\n      padding: 10px;\n      right: 0;\n      top: 0;\n      cursor: pointer;\n      background: transparent;\n      border: 0;\n      border-radius: 0;\n      box-shadow: none;\n      display: inline-block;\n      height: 44px;\n      vertical-align: top;\n      visibility: inherit;\n      width: 44px;\n      \n      &:hover {\n        opacity: 1;\n      }\n      \n      &:focus {\n        outline: none;\n      }\n    }\n  }\n}\n</style>"],"sourceRoot":"src/views"}]}